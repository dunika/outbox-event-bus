{
  "name": "@outbox-event-bus/postgres-drizzle-outbox",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run --exclude \"**/*.e2e.test.ts\"",
    "test:e2e": "docker-compose up -d --wait && vitest run src/e2e.test.ts; status=$?; docker-compose down; exit $status"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "outbox-event-bus": "workspace:*",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@outbox-event-bus/config": "workspace:*",
    "@types/node": "catalog:",
    "drizzle-kit": "^0.31.8",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
