    "format:check": "biome check .",
    "test:e2e:all": "./scripts/test-all-e2e.sh"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-vitest": "0.5.4",
    "tsdown": "^0.18.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite-tsconfig-paths": "^6.0.2",
    "vitest": "^4.0.16"
  }
