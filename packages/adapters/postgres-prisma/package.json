{
  "name": "@outbox-event-bus/postgres-prisma-outbox",
  "version": "1.0.3",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dunika/outbox-event-bus.git",
    "directory": "packages/adapters/postgres-prisma"
  },
  "homepage": "https://github.com/dunika/outbox-event-bus/tree/main/packages/adapters/postgres-prisma#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "prisma generate && tsdown",
    "test": "prisma generate && vitest run --passWithNoTests",
    "test:e2e": "export DATABASE_URL=postgresql://test_user:test_password@localhost:5434/outbox_test && docker-compose up -d --wait && prisma db push --accept-data-loss && vitest run -c vitest.e2e.config.ts; status=$?; docker-compose down -v; exit $status"
  },
  "dependencies": {
    "outbox-event-bus": "workspace:*"
  },
  "peerDependencies": {
    "@prisma/client": "^7.2.0"
  },
  "devDependencies": {
    "@outbox-event-bus/config": "workspace:*",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/node": "catalog:",
    "@types/pg": "^8.16.0",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "tsdown": "catalog:",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
