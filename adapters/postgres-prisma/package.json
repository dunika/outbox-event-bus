{
  "name": "@outbox-event-bus/postgres-prisma-outbox",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dunika/outbox-event-bus.git",
    "directory": "adapters/postgres-prisma"
  },
  "homepage": "https://github.com/dunika/outbox-event-bus/tree/main/adapters/postgres-prisma#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "prisma generate && tsdown",
    "test": "prisma generate && vitest run --exclude \"**/*.e2e.test.ts\" --exclude \"src/transactions.test.ts\"",
    "test:e2e": "docker-compose up -d --wait && vitest run src/e2e.test.ts; status=$?; docker-compose down; exit $status"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "outbox-event-bus": "workspace:*"
  },
  "devDependencies": {
    "@outbox-event-bus/config": "workspace:*",
    "@prisma/adapter-pg": "^7.2.0",
    "@types/node": "catalog:",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
